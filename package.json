  "dependencies": {
    "babel-cli": "^6.26.0",
    "browser-sync": "^2.23.6",
    "cli-table": "^0.3.1",
    "pug-cli": "^1.0.0-alpha6"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
